@article{belishev_algebras_2017-2,
	title = {On algebras of harmonic quaternion fields in \$\{{\textbackslash}mathbb {R}\}{\textasciicircum}3\$},
	url = {http://arxiv.org/abs/1710.00577},
	abstract = {Let \$\{{\textbackslash}mathscr A\}(D)\$ be an algebra of functions continuous in the disk \$D={\textbackslash}\{z{\textbackslash}in\{{\textbackslash}mathbb C\}{\textbackslash},{\textbar}{\textbackslash},{\textbackslash},{\textbackslash},{\textbar}z{\textbar}{\textbackslash}leqslant 1{\textbackslash}\}\$ and \{{\textbackslash}it holomorphic\} into \$D\$. The well-known fact is that the set \$\{{\textbackslash}mathscr M\}\$ of its characters (homomorphisms \$\{{\textbackslash}mathscr A\}(D){\textbackslash}to{\textbackslash}mathbb C\$) is exhausted by the Dirac measures \${\textbackslash}\{{\textbackslash}delta\_\{z\_0\}{\textbackslash},{\textbar}{\textbackslash},{\textbackslash},z\_0{\textbackslash}in D{\textbackslash}\}\$ and a homeomorphism \$\{{\textbackslash}mathscr M\}{\textbackslash}cong D\$ holds. We present a 3d analog of this classical result as follows. Let \$B={\textbackslash}\{x{\textbackslash}in\{{\textbackslash}mathbb R\}{\textasciicircum}3{\textbackslash},{\textbar}{\textbackslash},{\textbackslash},{\textbar}x{\textbar}{\textbackslash}leqslant 1{\textbackslash}\}\$. A quaternion field is a pair \$p={\textbackslash}\{{\textbackslash}alpha,u{\textbackslash}\}\$ of a function \${\textbackslash}alpha\$ and vector field \$u\$ in the ball \$B\$. A field \$p\$ is \{{\textbackslash}it harmonic\} if \${\textbackslash}alpha, u\$ are continuous in \$B\$ and \${\textbackslash}nabla{\textbackslash}alpha=\{{\textbackslash}rm rot{\textbackslash},\}u,{\textbackslash},\{{\textbackslash}rm div{\textbackslash},\}u=0\$ holds into \$B\$. The space \$\{{\textbackslash}mathscr Q\}(B)\$ of such fields is not an algebra w.r.t. the relevant (point-wise quaternion) multiplication. However, it contains the commutative algebras \$\{{\textbackslash}mathscr A\}\_{\textbackslash}omega(B)={\textbackslash}\{p{\textbackslash}in\{{\textbackslash}mathscr Q\}(B){\textbackslash},{\textbar}{\textbackslash},{\textbackslash},{\textbackslash}nabla\_{\textbackslash}omega{\textbackslash}alpha=0,{\textbackslash},{\textbackslash}nabla\_{\textbackslash}omega u=0{\textbackslash}\}{\textbackslash},{\textbackslash},({\textbackslash}omega{\textbackslash}in S{\textasciicircum}2)\$, each \$\{{\textbackslash}mathscr A\}\_{\textbackslash}omega(B)\$ being isometrically isomorphic to \$\{{\textbackslash}mathscr A\}(D)\$. This enables one to introduce a set \$\{{\textbackslash}mathscr M\}{\textasciicircum}\{{\textbackslash}mathbb H\}\$ of the \${\textbackslash}mathbb H\$-valued linear functionals on \$\{{\textbackslash}mathscr Q\}(B)\$ (\{{\textbackslash}it \${\textbackslash}mathbb H\$-characters\}), which are multiplicative on each \$\{{\textbackslash}mathscr A\}\_{\textbackslash}omega(B)\$, and prove that \$\{{\textbackslash}mathscr M\}{\textasciicircum}\{{\textbackslash}mathbb H\}={\textbackslash}\{{\textbackslash}delta{\textasciicircum}\{{\textbackslash}mathbb H\}\_\{x\_0\}{\textbackslash},{\textbar}{\textbackslash},{\textbackslash},x\_0{\textbackslash}in B{\textbackslash}\}{\textbackslash}cong B\$, where \${\textbackslash}delta{\textasciicircum}\{{\textbackslash}mathbb H\}\_\{x\_0\}(p)=p(x\_0)\$.},
	urldate = {2020-10-07},
	journal = {arXiv:1710.00577 [math]},
	author = {Belishev, Mikhail I. and Vakulenko, Aleksei F.},
	month = oct,
	year = {2017},
	note = {arXiv: 1710.00577},
	keywords = {30F15, 35Qxx, 46Jxx, Mathematics - Functional Analysis},
	file = {arXiv Fulltext PDF:C\:\\Users\\Colin\\Zotero\\storage\\3WP8B9B9\\Belishev and Vakulenko - 2017 - On algebras of harmonic quaternion fields in \$ ma.pdf:application/pdf;arXiv.org Snapshot:C\:\\Users\\Colin\\Zotero\\storage\\ZNX45KF7\\1710.html:text/html}
}

