	author = {Paiva, J. C. Álvarez and Fernandes, E.},
	month = feb,
	year = {2008},
	pages = {369},
	file = {Springer Full Text PDF:C\:\\Users\\Colin\\Zotero\\storage\\DUYMBMA6\\Paiva and Fernandes - 2008 - Gelfand transforms and Crofton formulas.pdf:application/pdf}
}

