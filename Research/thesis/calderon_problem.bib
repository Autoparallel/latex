	keywords = {Regular Function, Banach Algebra, Clifford Algebra, Dirac Operator, Symbol Mapping},
	pages = {1--26},
	file = {Springer Full Text PDF:C\:\\Users\\Colin\\Zotero\\storage\\BAZMIWY4\\Martin - 2015 - Function Spaces in Quaternionic and Clifford Analy.pdf:application/pdf}
}

